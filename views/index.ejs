<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>GETFYT | VENDOR</title>
  <!-- Tell the browser to be responsive to screen width -->
  <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
  <!-- Bootstrap 3.3.7 -->
  <link rel="stylesheet" href="../stylesheets/css/bootstrap.min.css">
  <!-- Theme style -->
  <link rel="stylesheet" href="../stylesheets/css/AdminLTE.min.css">
  <link rel="stylesheet" href="../stylesheets/css/_all-skins.min.css">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">

  <!-- Google Font -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700,300italic,400italic,600italic">
</head>
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  <header class="main-header">
    <!-- Logo -->
    <a href="#" class="logo" style="text-decoration:none">
      <span class="logo-lg"><b>GETFYT</b></span>
    </a>
    <!-- Header Navbar: style can be found in header.less -->
    <nav class="navbar navbar-static-top">
      <!-- Sidebar toggle button-->
      <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
        <i class="fas fa-bars"></i>
      </a>

      <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">

          <li class="dropdown user user-menu">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <img src="https://www.worldcrunch.com/assets/img/avatars/thumbnails/default-user-img-profile.jpg" class="user-image" alt="User Image">
              <span class="hidden-xs"><%= user.name %></span>
            </a>
            <ul class="dropdown-menu">
              <!-- User image -->
              <li class="user-header">
                <img src="https://www.worldcrunch.com/assets/img/avatars/thumbnails/default-user-img-profile.jpg" class="img-circle" alt="User Image">

                <p>
                  <%= user.name %> - MuscleGarage
                  <small>Member since Nov. 2012</small>
                </p>
              </li>

              <!-- Menu Footer-->
              <li class="user-footer">
                <div class="pull-left">
                  <a href="#" class="btn btn-default btn-flat">Profile</a>
                </div>
                <div class="pull-right">
                  <a href="/logout" class="btn btn-default btn-flat">Sign out</a>
                </div>
              </li>
            </ul>
          </li>
          <!-- Control Sidebar Toggle Button -->

        </ul>
      </div>
    </nav>
  </header>
  <!-- Left side column. contains the logo and sidebar -->
  <aside class="main-sidebar">
    <!-- sidebar: style can be found in sidebar.less -->
    <section class="sidebar">
      <!-- Sidebar user panel -->
      <div class="user-panel">
        <div class="pull-left image">
          <img src="https://www.worldcrunch.com/assets/img/avatars/thumbnails/default-user-img-profile.jpg" class="img-circle" alt="User Image">
        </div>
        <div class="pull-left info" style="padding-top:15px;text-transform:uppercase">
          <p><%= user.name %> </p>
        </div>
      </div>

      <!-- sidebar menu: : style can be found in sidebar.less -->
      <ul class="sidebar-menu" data-widget="tree">
        <li class="header">MAIN NAVIGATION</li>
        <li>
          <a href="/vendor/dashboard">
            <i class="fas fa-desktop"></i> <span>Dashboard</span>
          </a>
        </li>
        <li>
          <a href="/vendor/otpverification">
            <i class="fas fa-check-circle"></i> <span>Verify OTP</span>
          </a>
        </li>
        <li class="treeview">
          <a href="#">
            <i class="fas fa-building"></i>
            <span>Booking</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="/vendor/currentbookings"><i class="fas fa-circle-notch"></i> Current Booking</a></li>
            <li><a href="/vendor/futurebookings"><i class="fas fa-circle-notch"></i> Future Booking</a></li>
            <li><a href="/vendor/pastbookings"><i class="fas fa-circle-notch"></i> Past Booking</a></li>
            <li><a href="/vendor/consumedbookings"><i class="fas fa-circle-notch"></i> Consumed Booking</a></li>
          </ul>
        </li>
        <li>
          <a href="/vendor/profile">
            <i class="fas fa-user"></i> <span>Profile</span>
          </a>
        </li>
        <li>
          <a onclick="signout()" style="cursor:pointer;">
            <i class="fas fa-sign-out-alt"></i> <span>Sign Out</span>
          </a>
        </li>
      </ul>
    </section>
    <!-- /.sidebar -->
  </aside>

  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">

    </section>



    <!-- Main content -->
    <section class="content" style="margin-top:-3%;">
      <div id="page-wrapper" >
            <div id="page-inner">
                <div class="row" style="margin-bottom:-20px;">
                    <div class="col-md-12">
                     <h2><i class="fa fas fa-desktop fa-2x"></i> &nbsp;&nbsp;Dashboard</h2>
                    </div>
                </div>
                 <!-- /. ROW  -->
                 <hr />

            <div class="row">
                <div class="col-md-12">
                    <!-- Advanced Tables -->
                    <div class="panel panel-default">
                        <div class="panel-heading">
                         All Projects
                        </div>
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>S.No</th>
                                            <th>Username</th>
                                            <th>User Phone</th>
                                            <th>User Email</th>
                                            <th>Booking ID</th>
                                            <th>Book Date</th>
                                            <th>Pass Status</th>
                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr class="gradeU">
                                            <td>1</td>
                                            <td>James Stark</td>
                                            <td>7871581846</td>
                                            <td>jamesstark145@gmail.com</td>
                                            <td>78956</td>
                                            <td>10/02/2018</td>
                                            <td>Pending</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                    <!--End Advanced Tables -->
                </div>
            </div>
                <!-- /. ROW  -->

                </div>
            </div>
    </section>
    <!-- /.content -->
  </div>


  <!-- Add the sidebar's background. This div must be placed
       immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>
</div>
<!-- ./wrapper -->

<!-- jQuery 3 -->
<script src="../stylesheets/js/jquery.min.js"></script>
<!-- jQuery UI 1.11.4 -->
<script src="../stylesheets/js/jquery-ui.min.js"></script>
<!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
<script>
$.widget.bridge("uibutton", $.ui.button);
</script>
<!-- Bootstrap 3.3.7 -->
<script src="../stylesheets/js/bootstrap.min.js"></script>
<!-- Morris.js charts -->
<script src="../stylesheets/js/raphael.min.js"></script>
<script src="../stylesheets/js/morris.min.js"></script>
<!-- Sparkline -->
<script src="../stylesheets/js/jquery.sparkline.min.js"></script>
<!-- jvectormap -->
<script src="../stylesheets/js/jquery-jvectormap-1.2.2.min.js"></script>
<script src="../stylesheets/js/jquery-jvectormap-world-mill-en.js"></script>
<!-- jQuery Knob Chart -->
<script src="../stylesheets/js/jquery.knob.min.js"></script>
<!-- daterangepicker -->
<script src="../stylesheets/js/moment.min.js"></script>
<script src="../stylesheets/js/daterangepicker.js"></script>
<!-- datepicker -->
<script src="../stylesheets/js/bootstrap-datepicker.min.js"></script>
<!-- Bootstrap WYSIHTML5 -->
<script src="../stylesheets/js/bootstrap3-wysihtml5.all.min.js"></script>
<!-- Slimscroll -->
<script src="../stylesheets/js/jquery.slimscroll.min.js"></script>
<!-- FastClick -->
<script src="../stylesheets/js/fastclick.js"></script>
<!-- AdminLTE App -->
<script src="../stylesheets/js/adminlte.min.js"></script>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<script src="../stylesheets/js/dashboard.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="../stylesheets/js/demo.js"></script>

<!-- DataTables -->
<script src="../stylesheets/js/jquery.dataTables.js"></script>
<script src="../stylesheets/js/dataTables.bootstrap.js"></script>
<script defer src="https://use.fontawesome.com/releases/v5.0.6/js/all.js"></script>


<script>
function signout() {
  document.cookie = "token=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;";
  window.location.href = "/vendor";
}
$(document).ready(function() {
  $("#dataTables-example").dataTable();
});
</script>
</body>
</html>
